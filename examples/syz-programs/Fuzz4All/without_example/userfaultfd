r0 = userfaultfd(UFFDIO_API, 0)
ioctl(r0, UFFDIO_API, UFFDIO_REGISTER)
r1 = alloc(PAGE_SIZE)
mmap(r1, PAGE_SIZE, PROT_READ | PROT_WRITE, MAP_PRIVATE | MAP_ANONYMOUS, -1, 0)
ioctl(r0, UFFDIO_API, UFFDIO_COPY)
r2 = read(r1, PAGE_SIZE)
ioctl(r0, UFFDIO_API, UFFDIO_REMAP)
ioctl(r0, UFFDIO_API, UFFDIO_UNREGISTER)
close(r0)
